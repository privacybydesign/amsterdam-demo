{
    "name": "server",
    "version": "1.0.0",
    "description": "IRMA Demo node server",
    "main": "src/app.ts",
    "scripts": {
        "start": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./src/app.ts",
        "build": "tsc --build --pretty && tscpaths -p tsconfig.json -s ./src -o ./dist",
        "format": "prettier --write 'src/**/*.{ts,js}'",
        "lint": "eslint --fix",
        "test": "echo \"Error: no test specified\" && exit 0",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@privacybydesign/irma-backend": "^0.1.4",
        "@privacybydesign/irma-jwt": "^0.1.1",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-http-proxy": "^1.5.1",
        "express-session": "^1.17.1",
        "helmet": "^4.4.1",
        "http-proxy-middleware": "^2.0.0",
        "pg-promise": "^10.0.0",
        "reflect-metadata": "^0.1.13",
        "session-file-store": "^1.5.0",
        "tslib": "^2.1.0",
        "typedi": "^0.10.0",
        "uuid": "^3.3.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.3",
        "@types/helmet": "^4.0.0",
        "@types/http-proxy-middleware": "^1.0.0",
        "@types/node": "^14.14.32",
        "@types/session-file-store": "^1.2.1",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.17.0",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "fast-glob": "^3.2.6",
        "glob-parent": "^5.1.2",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
